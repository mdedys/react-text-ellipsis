{
  "name": "react-text-trimmer",
  "version": "2.0.0",
  "description": "React component that trims overflowing multi line text and adds a specified tail",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf lib",
    "prebuild": "yarn run clean && mkdirp lib",
    "build": "babel ./src -d ./lib",
    "test": "yarn run build && jest",
    "start": "webpack-dev-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdedys/react-text-trimmer.git"
  },
  "keywords": [
    "trim",
    "ellipsis",
    "react"
  ],
  "author": "Michal Dedys",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mdedys/react-text-trimmer/issues"
  },
  "homepage": "https://github.com/mdedys/react-text-trimmer#readme",
  "peerDependencies": {
    "prop-types": ">= 15.0.0",
    "react": ">= 15.0.0",
    "react-dom": ">= 15.0.0",
    "shortid": ">= 2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "canvas-prebuilt": "^1.6.5-prerelease.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "jsdom": "^11.4.0",
    "mkdirp": "^0.5.1",
    "mocha": "^4.0.1",
    "path": "^0.12.7",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-hot-loader": "^3.1.3",
    "rimraf": "^2.6.2",
    "shortid": "^2.2.8",
    "should": "^13.1.3",
    "should-sinon": "^0.0.6",
    "sinon": "^4.1.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "jest": {
    "testRegex": ".*-test.js"
  }
}
